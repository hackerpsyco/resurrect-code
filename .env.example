# ResurrectCI Environment Configuration
# Copy this file to .env and fill in your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_publishable_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# GitHub Integration
GITHUB_TOKEN=ghp_your_github_personal_access_token_here

# Vercel Integration
VERCEL_TOKEN=your_vercel_api_token_here

# AI Services
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Kestra Configuration (FREE MODE - NO TOKEN REQUIRED)
VITE_KESTRA_URL=http://localhost:8080
VITE_KESTRA_NAMESPACE=resurrectci
# VITE_KESTRA_API_TOKEN=your_token_here  # Optional - only for admin features

# CodeRabbit Configuration (Optional - GitHub App preferred)
CODERABBIT_API_KEY=your_coderabbit_api_key_here
CODERABBIT_ORG_ID=your_organization_id_here

# Development
NODE_ENV=development
VITE_APP_NAME=ResurrectCI
VITE_APP_VERSION=1.0.0